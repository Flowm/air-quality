    {
        "id": "16aa8a81.805e45",
        "type": "function",
        "z": "a4031254.d54bd",
        "name": "Temp & RH Analyzer",
        "func": "var rh = msg.payload.rh / 100;\nvar temp = msg.payload.temp;\n\nvar dewPoint = Math.pow(rh, 1 / 8) * (112 + 0.9 * temp) + 0.1 * temp - 112;\nvar expIndex = 5417.753 * ((1 / 273.16) - (1 / (dewPoint + 273.16)));\nvar humidex = temp + 0.5555 * (6.11 * Math.exp(expIndex) - 10);\n\n//Convert to 1-5 scala\nvar integer = parseInt(humidex / 10) - 1;\nif (integer < 0) {\n    integer = 0;\n}\nvar decimal = humidex % 10;\nvar convertedValue = integer + (decimal / 10);\n\nmsg.payload.raw = humidex;\nmsg.payload.type = 'trh';\nmsg.payload.processed = convertedValue;\n\nreturn msg;",
        "outputs": "1",
        "noerr": 0,
        "x": 768,
        "y": 438,
        "wires": [
