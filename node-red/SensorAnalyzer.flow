    {
        "id": "d83bc164.ed444",
        "type": "function",
        "z": "a4031254.d54bd",
        "name": "Value Join",
        "func": "var trh = context.get('trh')||0;\nvar mq = context.get('mq')||0;\n\nif (msg.payload.type == 'trh') {\n    trh = msg.payload;\n    delete trh.type;\n    context.set('trh', trh);\n} else if (msg.payload.type == 'mq') {\n    mq = msg.payload;\n    delete mq.type;\n    context.set('mq', mq);\n}\n\nmsg = {};\nmsg.payload = {};\nmsg.payload.trh = trh;\nmsg.payload.mq = mq;\nmsg.payload.max = Math.max(trh.processed, mq.processed);\nif (trh.processed > mq.processed) {\n    msg.payload.type = \"trh\";\n} else {\n    msg.payload.type = \"mq\";\n}\n\nreturn msg;",
